#include <stdio.h>

int main()
{
    int row,col;
    scanf("%d %d",&row , &col);
    int arr[row][col];
    for(int i=0;i<=row-1;++i){
        for(int j=0;j<=col-1;++j){
            scanf("%d",&arr[i][j]);
        }
    }
     for(int i=0;i<=row-1;++i){
         int sum=0;
        for(int j=0;j<=col-1;++j){
            sum+=arr[i][j];
        }
        printf("%d ",sum);
    }
    
    // printf("Hello World");

    return 0;
}
