#include <stdio.h>

int main()
{
    int n;
    int pro=1;
    scanf("%d",&n);
    int no=n;
    int square=n*n;
    while(n!=0){
        pro*=10;
        n=n/10;
        
    }
    if(no==square%pro)
    printf("%d is automorphic",no);
    else
    printf("%d is not automorphic",no);
    //printf("Hello World");
    return 0;
}
