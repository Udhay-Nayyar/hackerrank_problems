#include <stdio.h>

int main() {
    int n;
    int odd=0,even=0;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<=n-1;++i){
        scanf("%d",&a[i]);
    }
    for(int i=0;i<=n-1;++i){
        if(((a[i])&1)==1){
            odd++;
        }
        else{
            even++;
        }
    }

    printf("%d %d",even,odd);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
